    "@types/nodemailer": "^6.4.0",
    "@types/websocket": "^1.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "handlebars": "^4.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.4",
    "nodemailer": "^6.4.4",
    "rabbitmq-event-manager": "^1.1.1",
    "rxjs": "^6.5.4",
    "text-encoding": "^0.7.0",
    "validator": "^12.2.0",
    "websocket": "^1.0.31",
    "ws": "^7.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
