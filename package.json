  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "dev": "nodemon  -e ts -w ./src --exec node -r ts-node/register src/index.ts",
    "start": "node build/index.js",
    "lint": "tslint src/**/*.ts test/**/*.ts",
    "format": "prettier --check \"**/*.ts\"",
